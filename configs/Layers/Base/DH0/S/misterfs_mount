SET uname `ags_uname`

assign >NIL: EXISTS MiSTer:
if WARN
  if $uname EQ "mister"
    mount MR0:
  else
    makedir RAM:MiSTer >NIL:
    assign MiSTer: RAM:MiSTer
    skip DONE
  endif
endif

assign >NIL: EXISTS MiSTer:
if NOT WARN
  if NOT EXISTS MiSTer:Disk.info
    copy >NIL: SYS:Prefs/Icons/MiSTer/Disk.info MiSTer:
  endif
endif

LAB DONE
